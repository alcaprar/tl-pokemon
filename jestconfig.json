{
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "testMatch": [
    "**/__tests__/**/*.[jt]s?(x)",
    "**/?(*.)+(spec|test).[jt]s?(x)"
  ],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "roots": [
    "./src"
  ],
  "reporters": [
    "default"
  ]
}
